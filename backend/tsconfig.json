{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": ".vercel/output",
    "rootDir": ".",
    "resolveJsonModule": true,
    "declaration": false,
    "allowSyntheticDefaultImports": true
  },
  "include": ["api/**/*"],
  "exclude": ["node_modules", "src", "local"]
}
